.index-swiper swiper image {
  width: 100%;
  height: 100%;
}
.index-cate {
  display: flex;
}
.index-cate navigator {
  padding: 20rpx;
  flex: 1;
}
.index-cate navigator image {
  width: 100%;
}
.index-floor .floor-group {
  padding: 10rpx 0;
}
.index-floor .floor-group .floor-title image {
  width: 100%;
}
