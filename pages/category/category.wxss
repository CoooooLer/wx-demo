page {
  height: 100%;
}
.cate {
  height: 100%;
}
.cate .cate-container {
  height: calc(100vh - 90rpx);
  display: flex;
}
.cate .cate-container .left-view {
  flex: 2;
}
.cate .cate-container .left-view .menu-item {
  display: flex;
  height: 80rpx;
  align-items: center;
  justify-content: center;
  font-size: 30rpx;
}
.cate .cate-container .left-view .active {
  color: var(--themeColor);
  border-left: 5rpx solid currentColor;
}
.cate .cate-container .right-content {
  flex: 5;
}
.cate .cate-container .right-content .goods-group .goods-title {
  display: flex;
  align-items: center;
  justify-content: center;
  height: 80rpx;
}
.cate .cate-container .right-content .goods-group .goods-title .delimiter {
  color: #ccc;
  padding: 0 10rpx;
}
.cate .cate-container .right-content .goods-group .goods-title .title {
  color: #333;
}
.cate .cate-container .right-content .goods-group .goods-list {
  display: flex;
  flex-wrap: wrap;
}
.cate .cate-container .right-content .goods-group .goods-list navigator {
  width: 33.33%;
  text-align: center;
}
.cate .cate-container .right-content .goods-group .goods-list navigator image {
  width: 50%;
}
.cate .cate-container .right-content .goods-group .goods-list navigator .goods-name {
  font-size: 36rpx;
}
